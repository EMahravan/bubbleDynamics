FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      sampleDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

interpolationScheme cellPoint;

setFormat       raw;

surfaceFormat   vtk;

sets
(
    line
    {
        type    face;
        axis    x;
        start   ( XMIN 0 0 );
        end     ( XMAX 0 0 );
        nPoints 500;
    }
);

surfaces        ();

fields          ( p T rho U);


